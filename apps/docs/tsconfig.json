{
	"extends": "@turbopandarepo/tsconfig/nextjs.json",
	"compilerOptions": {
		"outDir": "dist",
		"baseUrl": ".",
		"paths": {
			"~/*": ["./src/*"]
		}
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		".storybook/**/*.ts",
		"node_modules/@turbopandarepo/ds"
	],
	"exclude": ["node_modules", "dist"]
}
