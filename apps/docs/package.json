{
	"name": "@turbopandarepo/docs",
	"version": "0.0.0",
	"private": true,
	"description": "NextJs featuring PandaCSS and Storybook",
	"license": "MIT",
	"scripts": {
		"build": "next build",
		"clean": "rimraf .turbo node_modules .next",
		"dev": "rimraf .next && next dev -p 3002",
		"lint": "eslint \"{src,.storybook}/**/*.ts*\"",
		"prepare": "panda codegen --clean",
		"start": "next start ",
		"storybook:build": "storybook build --quiet",
		"storybook:dev": "rimraf .next && storybook dev -p 6006",
		"storybook:move": "mv storybook-static ../../storybook-static"
	},
	"dependencies": {
		"@icons-pack/react-simple-icons": "^9.0.1",
		"@pandacss/preset-panda": "^0.15.4",
		"@park-ui/presets": "^0.16.0",
		"@radix-ui/colors": "3.0.0",
		"@turbopandarepo/preset": "workspace:*",
		"@turbopandarepo/ui": "workspace:*",
		"@vercel/analytics": "^1.0.2",
		"animated-pandacss": "^0.0.2",
		"lucide-react": "^0.284.0",
		"next": "^13.5.4",
		"next-seo": "^6.1.0",
		"panda-transitions-css": "^0.0.1",
		"pandacss-preset-radix-colors": "^0.0.7",
		"pandacss-preset-typography": "^0.1.3",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-preset-env": "^9.1.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"schema-dts": "^1.1.2"
	},
	"devDependencies": {
		"@pandacss/dev": "^0.15.4",
